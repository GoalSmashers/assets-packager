{
  "name": "assets-packager",
  "author": "Jakub Pawlowicz <jakub@goalsmashers.com> (http://twitter.com/GoalSmashers)",
  "description": "Very fast assets packager - combines power of seq, uglify-js, gzip, assets-expander, clean-css and couple other tools to build assets insanely fast",
  "keywords": ["assets", "packager", "css", "javascript", "less", "minifier"],
  "homepage": "http://github.com/GoalSmashers/assets-packager",
  "repository": {
    "type" : "git",
    "url" : "http://github.com/GoalSmashers/assets-packager.git"
  },
  "version": "0.9.0",
  "bin": {
    "assetspkg": "./bin/assetspkg"
  },
  "scripts": {
    "check": "jshint .",
    "prepublish": "jshint .",
    "test": "vows"
  },
  "dependencies": {
    "optimist": "0.5.x",
    "seq": "0.3.x",
    "uglify-js": "1.2.x",
    "less": "1.x",
    "clean-css": "1.1.x",
    "enhance-css": "0.6.x",
    "assets-expander": "0.5.x"
  },
  "devDependencies": {
    "jshint": "2.1.11",
    "vows": "0.7.x"
  },
  "jshintConfig": {
    "browser": false,
    "camelcase": true,
    "curly": false,
    "eqeqeq": false,
    "eqnull": false,
    "immed": true,
    "latedef": true,
    "maxerr": 100,
    "multistr": true,
    "noarg": true,
    "node": true,
    "plusplus": false,
    "quotmark": "true",
    "regexp": false,
    "strict": false,
    "trailing": true,
    "undef": true,
    "unused": true
  },
  "engines": {
    "node": ">=0.6.0"
  }
}
